{
	"compilerOptions": {
		"allowJs": false,
		"removeComments": true,
		"module": "commonjs",
		"target": "es6",
		"outDir": "dist",
		"rootDir": ".",
		"sourceMap": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"noImplicitReturns": true
	},
	"exclude": ["node_modules", "**/*.test.ts"]
}
